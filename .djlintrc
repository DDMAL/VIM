{
  "exclude": [
    "migrations/*"
  ],
  "extension": [
    "html"
  ],
  "indent": 2,
  "blank_line_after_tag": "load,extends,include,endblock",
  "ignore": "H006,H016,H021,H025,H030,H031",
  "close_void_tags": true,
  "format_css": true,
  "css": {
    "indent_size": 2
  },
  "format_js": true,
  "js": {
    "indent_size": 2
  }
}
